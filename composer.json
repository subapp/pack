{
  "name": "subapp/pack",
  "description": "Subapp Data Pack",
  "keywords": [
    "php data pack", "data pack"
  ],
  "authors": [
    {"name": "Ivan Hontarenko"}
  ],
  "license": "MIT",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-msgpack": "*",
    "ext-bz2": "*",
    "ext-zlib": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "Subapp\\Pack\\": "src/",
      "Subapp\\Pack\\App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Subapp\\Pack\\Tests\\": "tests/"
    }
  }
}
